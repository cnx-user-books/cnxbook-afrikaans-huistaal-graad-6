    <section id="id2649320">
      <title>DIE KARAKTERVOLLE HUISIE</title>
      <figure id="id1169047289653">
        <media id="id1169047289653_media" alt="">
          <image mime-type="image/png" src="../../media/Picture 1-df8e.png" id="id1169047289653__onlineimage" height="312" width="552"/>
